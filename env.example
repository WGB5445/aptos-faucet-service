# 数据库配置
FAUCET__DATABASE__KIND=postgres
FAUCET__DATABASE__URL=postgres://username:password@localhost:5432/faucet

# OAuth 配置
FAUCET__AUTH__GOOGLE_CLIENT_ID=your-google-client-id
# 注意：google_client_secret 不是必需的，因为后端只验证ID token
# FAUCET__AUTH__GOOGLE_CLIENT_SECRET=your-google-client-secret

# JWT 配置
FAUCET__JWT_SECRET=your-jwt-secret-key-change-this-in-production

# Bot Tokens
TELOXIDE_TOKEN=your-telegram-bot-token
DISCORD_TOKEN=your-discord-bot-token

# 可选：跳过数据库（用于测试）
FAUCET_NO_DB=false

# 可选：覆盖其他配置
# FAUCET__SERVER__HTTP_ADDR=0.0.0.0:8080
# FAUCET__LIMITS__DEFAULT_AMOUNT=1
